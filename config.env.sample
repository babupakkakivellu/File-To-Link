# Telegram API Credentials
# Get from https://my.telegram.org
API_ID=12345678
API_HASH=your_api_hash_here

# Main Bot Token
# Get from @BotFather
MAIN_BOT_TOKEN=1234567890:ABCdefGHIjklMNOpqrsTUVwxyz

# Worker Bot Tokens (for load balancing)
# Separate multiple tokens with commas
# Get additional tokens from @BotFather
WORKER_BOTS=1111111111:AAAbbbCCCdddEEEfff,2222222222:GGGhhhIIIjjjKKKlll,3333333333:MMMnnnOOOpppQQQrrr

# Dump Channel ID
# Create a private channel and add all bots as admins
# Get channel ID from @userinfobot (forward any message from the channel)
# Format: -1001234567890
DUMP_CHANNEL=-1001234567890

# Server Configuration
# Your public domain or IP address (no trailing slash)
BASE_URL=https://yourdomain.com
PORT=8000

# Owner ID (your Telegram user ID)
# Get from @userinfobot
OWNER_ID=123456789
